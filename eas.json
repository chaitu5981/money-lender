{
  "cli": {
    "version": ">= 13.2.8"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      }
    },
    "apk": {
      "android": { "buildType": "apk" }
    }
  },
  "submit": {
    "production": {}
  }
}
